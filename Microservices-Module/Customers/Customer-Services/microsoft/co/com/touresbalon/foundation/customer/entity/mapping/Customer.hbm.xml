<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Customer-Services"
                   namespace="Customer_Services.microsoft.co.com.touresbalon.foundation.customer.entity">

  <class name="Customer" table="customer" schema="dbo" catalog="customers">
    <id name="Id">
      <generator class="assigned"/>
    </id>
    <property name="first_name"/>
    <property name="last_name"/>
    <property name="phone_number"/>
    <property name="email"/>
    <property name="password"/>
    <property name="creditcard_type"/>
    <property name="creditcard_number"/>
    <property name="status"/>
  </class>

</hibernate-mapping>